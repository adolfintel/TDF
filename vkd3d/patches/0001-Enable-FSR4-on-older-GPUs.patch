From: Federico Dossena <info@fdossena.com>
Date: Tue, 18 Nov 2025 06:19:48 +0100
Subject: [PATCH] Enable FSR4 on older GPUs

---
 meson_options.txt | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/meson_options.txt b/meson_options.txt
index bac0610d..189478b5 100644
--- a/meson_options.txt
+++ b/meson_options.txt
@@ -3,5 +3,5 @@ option('enable_extras',             type : 'boolean', value : false)
 option('enable_profiling',          type : 'boolean', value : false)
 option('enable_renderdoc',          type : 'boolean', value : false)
 option('enable_descriptor_qa',      type : 'boolean', value : false)
-option('enable_extended_emulation', type : 'boolean', value : 'false')
+option('enable_extended_emulation', type : 'boolean', value : true)
 option('enable_trace',              type : 'combo',   value : 'auto', choices : ['false', 'true', 'auto'])
